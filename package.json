    "watch": "concurrently \"yarn dev\" \"yarn watch:data\"",
    "build": "yarn build:data && next build && next export"
  },
  "dependencies": {
    "@vimeo/player": "^2.15.0",
    "concurrently": "^5.3.0",
    "js-yaml": "^4.0.0",
    "next": "12.1.0",
    "nodemon": "^2.0.7",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  }
}
